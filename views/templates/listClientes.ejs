<h1><strong>Listado de clientes</strong></h1></br>
<div ng-controller="clienteController">
    <div class="container">
        <div class="row">
            <input class="form-control" type="search" id="search" placeholder="Filtrar clientes..." aria-label="Search" ng-model="clienteSearch"
                ng-keyup="searchCliente()">
        </div>
        <div class="row">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Nombre</th>
                        <th scope="col">CIF</th>
                        <th scope="col">Dirrecci√≥n</th>
                        <th scope="col">Grupo</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="customer in allCustomers">
                        <td>{{ customer.nombre }}</td>
                        <td>{{ customer.cif }}</td>
                        <td>{{ customer.direccion }}</td>
                        <td>{{ customer.grupo.nombre }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>